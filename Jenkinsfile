pipeline {
  agent any
  
  stages {
    stage('Run python script') {
      steps {
            echo 'Running Python script...'
//             echo '|' cd
            }
      }
    
    stage('Run Python script') {
      steps {
            echo 'python """\\ProgramData\\Jenkins\\.jenkins\\workspace\\Run Python Script\\main.py"""'
            sh 'python "\\ProgramData\\Jenkins\\.jenkins\\workspace\\Run Python Script\\main.py"'
            sh 'echo | cd'
            }
        }
    }
}
